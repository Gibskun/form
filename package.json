{
  "name": "multi-user-form-system",
  "version": "1.0.0",
  "description": "Multi-user form system with React frontend and Node.js backend",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && node server.js",
    "frontend": "cd frontend && npm start",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "init-db": "cd backend && node database.js",
    "dev": "npm run init-db && npm start"
  },
  "keywords": ["react", "nodejs", "postgresql", "forms"],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}